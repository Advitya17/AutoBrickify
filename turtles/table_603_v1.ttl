@prefix bldg: <http://example.org/building#> .
@prefix brick: <https://brickschema.org/schema/1.1/Brick#> .

bldg:VAV_Local_Application a brick:VAV ;
    brick:feeds bldg:Local_Application ;
    brick:hasPt <http://example.org/building#NAE-103/FCB.Local_Application.SA1-T>,
        <http://example.org/building#NAE-103/FCB.Local_Application.SA2-T>,
        <http://example.org/building#NAE-103/FCB.Local_Application.ZN1-SP>,
        <http://example.org/building#NAE-103/FCB.Local_Application.ZN1-T>,
        <http://example.org/building#NAE-103/FCB.Local_Application.ZN2-SP>,
        <http://example.org/building#NAE-103/FCB.Local_Application.ZN2-T> ;
    brick:isFedBy <http://example.org/building#AHU_NAE-103/FCB> .

<http://example.org/building#AHU_NAE-103/FCB> a brick:AHU .

bldg:Local_Application a brick:HVAC_Zone .

<http://example.org/building#NAE-103/FCB.Local_Application.SA1-T> a brick:Location .

<http://example.org/building#NAE-103/FCB.Local_Application.SA2-T> a brick:Location .

<http://example.org/building#NAE-103/FCB.Local_Application.ZN1-SP> a brick:Setpoint .

<http://example.org/building#NAE-103/FCB.Local_Application.ZN1-T> a brick:Zone .

<http://example.org/building#NAE-103/FCB.Local_Application.ZN2-SP> a brick:Setpoint .

<http://example.org/building#NAE-103/FCB.Local_Application.ZN2-T> a brick:Zone .

